{
  "name": "cdk",
  "version": "0.1.0",
  "bin": {
    "cdk": "bin/cdk.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "cdk:lambda:install": "cd ../lambda && npm i && cd ../cdk",
    "deploy": "npm run cdk:lambda:install && cdk deploy --require-approval never --outputs-file output.json"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.132",
    "@types/jest": "^29.5.11",
    "@types/node": "20.11.10",
    "aws-cdk": "2.124.0",
    "esbuild": "^0.20.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "~5.3.3"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.501.0",
    "@aws-sdk/lib-dynamodb": "^3.501.0",
    "aws-cdk-lib": "2.124.0",
    "constructs": "^10.3.0",
    "source-map-support": "^0.5.21"
  }
}
